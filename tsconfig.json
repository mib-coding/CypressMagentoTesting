{
        "compilerOptions": {
      "target": "es6",
      "lib": ["es6", "dom"],
      "allowJs": true,
      "checkJs": true,
      "outDir": "cypress/dist",
      "rootDir": "cypress",
      "strict": true,
      "esModuleInterop": true,
      "types": [ "cypress" ]  // Add this line

    },
    "include": 
    ["cypress/**/*.ts",
    "cypress/support/commands.d.ts" // Explicitly include the commands declaration file
    ],

    "exclude": ["node_modules"]

  }
  